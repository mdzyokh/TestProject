<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#f5f0e8"/>
        </shape>
    </item>
    <item>
        <bitmap android:src="@drawable/texture_background"
                android:tileMode="repeat"
                android:dither="true"/>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#88f5f0e8"
                      android:endColor="#66f5f0e8"
                      android:angle="-90">
            </gradient>
        </shape>
    </item>

</layer-list>